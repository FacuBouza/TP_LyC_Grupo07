   22 $@1: /* vacío */

   23 while: WHILE condicion DO bloque $@1 ENDWHILE

   24 $@2: /* vacío */

   25 while_especial: WHILE ID IN exp_while DO bloque $@2 ENDWHILE

   26 exp_while: OP_CORC list_exp CL_COR

   27 list_exp: expresion
   28         | list_exp OP_COMA expresion

   29 $@3: /* vacío */

   30 if: IF OP_PAR condicion CL_PAR bloque $@3 ENDIF

   31 $@4: /* vacío */

   32 if: IF OP_PAR condicion CL_PAR bloque ELSE bloque $@4 ENDIF

   33 condicion: comparacion_simple
